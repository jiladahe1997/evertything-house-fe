  "version": "0.2.0",
  "configurations": [
  
    {
      "type": "node",
      "request": "launch",
      "name": "node服务器调试",
      "program": "${workspaceFolder}/server/app.js"
    },

    {
      "type": "node",
      "request": "launch",
      "name": "发布调试",
      "program": "${workspaceFolder}/publishNodeServer.js",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}\\node_modules\\@vue\\cli-service\\bin\\vue-cli-service.js",
      "cwd": "${workspaceFolder}",
      "args": ["serve"]
    }
  ]
}